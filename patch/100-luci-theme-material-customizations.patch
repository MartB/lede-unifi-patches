From 7e147ab94eb589e6e1bc93fec742a017b6763a7b Mon Sep 17 00:00:00 2001
From: MartB <mart.b@outlook.de>
Date: Tue, 15 Aug 2017 06:01:55 +0200
Subject: [PATCH] #44 Corrected some invalid styling attributes (0% + X is not
 valid tested on google chrome 60.0.3112.101)

---
 files/htdocs/css/style.css | 8 +++-----
 1 file changed, 3 insertions(+), 5 deletions(-)

diff --git a/feeds/luci/themes/luci-theme-material/htdocs/luci-static/material/css/style.css b/feeds/luci/themes/luci-theme-material/htdocs/luci-static/material/css/style.css
index 545c5fb..2bdd6d2 100755
--- a/feeds/luci/themes/luci-theme-material/htdocs/luci-static/material/css/style.css
+++ b/feeds/luci/themes/luci-theme-material/htdocs/luci-static/material/css/style.css
@@ -206,9 +206,7 @@ footer > a {
 .main-left {
     float: left;
     top: 4rem;
-    width: 15%;
-    width: calc(0% + 15rem);
-    height: 100%;
+    width: 15rem;
     height: calc(100% - 4rem);
     background-color: white;
     overflow-x: auto;
@@ -1231,7 +1229,7 @@ body.lang_pl.node-main-login .cbi-value-title {
 
 @media screen and (max-width: 1600px) {
     .main-left {
-        width: calc(0% + 13rem);
+        width: calc(13rem);
     }
 
     .main-right {
@@ -1284,7 +1282,7 @@ body.lang_pl.node-main-login .cbi-value-title {
     }
 
     .main-left {
-        width: calc(0% + 13rem);
+        width: calc(13rem);
         top: 3.5rem;
         height: calc(100% - 3.5rem);
     }

diff --git a/feeds/luci/themes/luci-theme-material/htdocs/luci-static/material/css/style.css b/feeds/luci/themes/luci-theme-material/htdocs/luci-static/material/css/style.css
index 545c5fb..f9dbf5e 100755
--- a/feeds/luci/themes/luci-theme-material/htdocs/luci-static/material/css/style.css
+++ b/feeds/luci/themes/luci-theme-material/htdocs/luci-static/material/css/style.css
@@ -18,9 +18,6 @@
  *  Licensed to the public under the Apache License 2.0
  */
 
-/*
- *  Font generate by Icomoon<icomoon.io>
- */
 @font-face {
     font-family: 'icomoon';
     src: url('../fonts/font.eot');
@@ -162,29 +159,31 @@ footer > a {
     height: calc(100% - 4rem);
 }
 
-.main > .loading {
-    position: fixed;
+.page_loading {
+    position: absolute;
+    top: 0rem;
     width: 100%;
-    height: 100%;
+    height: 4rem;
+    line-height: 4rem;
     z-index: 1000;
     display: block;
     background-color: rgb(240, 240, 240);
-    top: 0;
-}
-
-.main > .loading > span {
     display: block;
     text-align: center;
-    margin-top: 2rem;
     color: #888;
     font-size: 1.2rem;
 }
 
-.main > .loading > span > .loading-img:before {
+.page_loading div {
+    display: inline;
+    vertical-align: middle;
+}
+
+.page_loading .loading-img:before {
     content: "\e603";
 }
 
-.main > .loading > span > .loading-img {
+.page_loading .loading-img {
     animation: anim-rotate 2s infinite linear;
     margin-right: 0.2rem;
     display: inline-block;
@@ -1270,6 +1269,10 @@ body.lang_pl.node-main-login .cbi-value-title {
 }
 
 @media screen and (max-width: 1280px) {
+    .page_loading {
+        height: 3.5rem;
+        line-height: 3.5rem;
+    }
     header {
         height: 3.5rem;
     }
